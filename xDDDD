import numpy as np
import matplotlib.pyplot as plt


x=np.linspace(-10,10,50)
y=[]
y1=[]
y2=[]
y3=[]

for i in x:
    a=(1*i)/(1+1**5*i**2)
    y.append(a)
plt.plot(y)

for i in x:
    a=(2*i)/(1+2**5*i**2)
    y1.append(a)
plt.plot(y1)

for i in x:
    a=(3*i)/(1+3**5*i**2)
    y2.append(a)
plt.plot(y2)

for i in x:
    a=(4*i)/(1+4**5*i**2)
    y3.append(a)
plt.plot(y3)

plt.show()
